{% extends template('widget') %}

 {% define data = {
     productSetList: _widget.productSetList
 } %}

{% block body %}
    {% if data.productSetList is defined %}
        {% for productSet in data.productSetList %}
            {% include molecule('product-set', 'ProductSetWidget') ignore missing with {
                data: {
                    products: productSet.productSet
                }
            } only %}
        {% endfor %}
    {% endif %}
{% endblock %}