{% extends model('component') %}

{% define config = {
    name: 'slick-carousel',
    tag: 'slick-carousel'
} %}

{% define data = {
    slides: []
} %}

{% define attributes = {
    'slider-config': '{}'
} %}

{% block class %}
    {{ parent() }} is-hidden
{% endblock %}

{% block body %}
    {% block slides %}
        {% for slide in data.slides %}
            {{ slide }}
        {% endfor %}
    {% endblock %}
{% endblock %}

