{% extends model('component') %}

{% define config = {
    name: 'date-time-picker',
    tag: 'date-time-picker'
} %}

{% define data = {
    field: required
} %}

{% define attributes = {
    language: 'date_time_picker_language' | trans,
    parent: '.' ~ config.name,
    config: '{}',
    'trigger-selector': required
} %}

{% block body %}
    {{ form_row(data.field) }}
{% endblock %}
